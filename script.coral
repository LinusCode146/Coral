let binarySearch = fn(arr, target, low, high) {
    if (low > high) {
        return -1;
    };

    let mid = (low + high) / 2;
    let midVal = arr[mid];

    if (midVal == target) {
        return mid;
    };

    if (midVal > target) {
        return binarySearch(arr, target, low, mid - 1);
    } else {
        return binarySearch(arr, target, mid + 1, high);
    }
};

let arr = [1, 3, 5, 7, 9, 11, 13, 17, 21];
let result = binarySearch(arr, 21, 0, 8);
log(result)